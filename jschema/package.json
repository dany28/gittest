{
  "name": "@witkac/jschema",
  "version": "5.7.0",
  "description": "Obsługa JSONSchema, nawet w przeglądarce",
  "repository": {
    "type": "git",
    "url": "https://gitlab.infocity.pl/witkac/jschema"
  },
  "author": "Daniel Jursza",
  "license": "ISC",
  "module": "JSchema.es.js",
  "typings": "JSchema.d.ts",
  "scripts": {
    "test": "vitest run",
    "publishnpm": "npm publish --registry https://npm.witkac.pl",
    "publishrc": "npm run build && npm publish --registry https://npm.witkac.pl --tag rc",
    "pack_old": "./node_modules/rollup/dist/bin/rollup --format iife --name \"JSchema\" --file ./dist/bundle.js",
    "pack:dev": "node ./node_modules/rollup/dist/bin/rollup -c ./rollup.config.mjs",
    "pack:prod": "node ./node_modules/rollup/dist/bin/rollup -c ./rollup.config.prod.mjs",
    "pack:dts": "node ./node_modules/rollup/dist/bin/rollup -c ./rollup.config.dts.mjs",
    "pack:dts.vue": "node ./node_modules/rollup/dist/bin/rollup -c ./rollup.config.dts.vue.mjs",
    "pack:prod.vue": "node ./node_modules/rollup/dist/bin/rollup -c ./rollup.config.prod.vue.mjs",
    "pack:prod.simple": "node ./node_modules/rollup/dist/bin/rollup -c ./rollup.config.prod.simple.mjs",
    "pack2": "esbuild --bundle ./src/index.ts --outfile=./dist/JSchema2.min.js '--define:process.env.NODE_ENV=\"production\"' --minify --platform=browser --format=iife --pure:console.log",
    "pack3": "esbuild --bundle ./src/index.ts --outfile=./dist/JSchema2.js --global-name=JSchema --platform=browser --format=iife --external:moment --pure:moment",
    "pack": "npm run pack:prod && npm run pack:dts && npm run pack:prod.simple && npm run pack:prod.vue",
    "build": "node ./node_modules/typescript/bin/tsc",
    "storybook": "storybook dev -p 6006",
    "prepublishOnly2": "npm run build && npm run pack:dev",
    "prepublishOnly_off": "npm run build && npm run pack",
    "fullPublish": "npm run publishnpm",
    "build-storybook": "storybook build",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "12.1.2",
    "@storybook/addon-essentials": "^8.5.8",
    "@storybook/addon-interactions": "^8.5.8",
    "@storybook/addon-links": "^8.5.8",
    "@storybook/blocks": "^8.5.8",
    "@storybook/test": "^8.5.8",
    "@storybook/vue3": "^8.5.8",
    "@storybook/vue3-vite": "^8.5.8",
    "@types/node": "^18.19.76",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "2.4.6",
    "@witkac/dev-tools": "2.0.0",
    "c8": "^10.1.2",
    "dayjs": "^1.11.13",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "html-webpack-plugin": "^5.6.3",
    "i18next": "^24.2.2",
    "i18next-vue": "^5.2.0",
    "jsdom": "^26.0.0",
    "json-schema-to-ts": "3.1.1",
    "rollup": "^4.34.8",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "storybook": "^8.5.8",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.25.0",
    "vitest": "^3.0.7",
    "vue": "^3.4.38"
  },
  "dependencies": {
    "reflect-metadata": "0.2.2"
  },
  "optionalDependencies": {
    "i18next": "^24.2.2",
    "i18next-vue": "^5.2.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  }
}
